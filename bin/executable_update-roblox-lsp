curl -o ~/.local/src/robloxlsp.zip https://nightrains.gallery.vsassets.io/_apis/public/gallery/publisher/nightrains/extension/robloxlsp/latest/assetbyname/Microsoft.VisualStudio.Services.VSIXPackage
unzip -d ~/.local/src/robloxlsp_temp ~/.local/src/robloxlsp.zip
rm -r ~/.local/src/robloxlsp
mv ~/.local/src/robloxlsp_temp/extension/server ~/.local/src/robloxlsp
chmod +777 ~/.local/src/robloxlsp/bin/Linux/lua-language-server
rm -r ~/.local/src/robloxlsp_temp
rm ~/.local/src/robloxlsp.zip
